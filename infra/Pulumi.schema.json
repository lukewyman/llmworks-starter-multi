{
    "name": "llmworks-starter-multi",
    "properties": {
        "awsRegion": {
            "type": "string"
        },
        "clusterName": {
            "type": "string"
        },
        "clusterVersion": {
            "type": "string",
            "default": "1.29"
        },
        "vpcCidr": {
            "type": "string",
            "default": "10.42.0.0/16"
        },
        "azs": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 2
        },
        "nodeDesiredSize": {
            "type": "integer",
            "default": 2
        },
        "nodeMinSize": {
            "type": "integer",
            "default": 1
        },
        "nodeMaxSize": {
            "type": "integer",
            "default": 3
        },
        "nodeInstanceTypes": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "default": [
                "t3.small"
            ]
        },
        "nodeCapacityType": {
            "type": "string",
            "enum": [
                "SPOT",
                "ON_DEMAND"
            ],
            "default": "SPOT"
        }
    },
    "required": [
        "awsRegion",
        "clusterName",
        "azs"
    ]
}